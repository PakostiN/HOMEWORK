
public class Zad4 {

	public static void main(String[] args) {
		System.out.println("Напишете програма, която принтира на екрана числата от 30 до 1 в обратен ред, подредени в 5 реда и 6 колони");
		int n = 30;
		
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 6; j++) {
				System.out.print(n-- + " ");
			}
			System.out.println(" ");
		}

	}

}
